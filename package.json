{
    "name": "uk-towns-explorer",
    "version": "1.0.0",
    "description": "Interactive UK Towns Map with Statistics and Chat Interface",
    "main": "index.html",
    "scripts": {
        "start": "http-server -p 8080",
        "dev": "http-server -p 8080 -c-1",
        "test": "jest",
        "build": "npm run build:css && npm run build:js",
        "build:css": "postcss css/*.css -d dist/css",
        "build:js": "webpack --mode production",
        "lint": "eslint js/**/*.js",
        "format": "prettier --write \"**/*.{js,css,html,json}\""
    },
    "dependencies": {
        "d3": "^7.8.5",
        "leaflet": "^1.9.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "^2.12.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.22.20",
        "@babel/preset-react": "^7.22.15",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "css-loader": "^6.8.1",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react": "^7.33.2",
        "http-server": "^14.1.1",
        "jest": "^29.7.0",
        "postcss": "^8.4.31",
        "postcss-cli": "^10.1.0",
        "prettier": "^3.0.3",
        "style-loader": "^3.3.3",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/uk-towns-explorer.git"
    },
    "keywords": [
        "uk",
        "maps",
        "data-visualization",
        "react",
        "d3",
        "leaflet",
        "chatbot"
    ],
    "author": "Your Name",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/yourusername/uk-towns-explorer/issues"
    },
    "homepage": "https://github.com/yourusername/uk-towns-explorer#readme"
}
